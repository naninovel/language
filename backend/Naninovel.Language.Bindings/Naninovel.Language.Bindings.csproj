<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageId>Naninovel.Language.Bindings</PackageId>
        <Version>$([System.DateTime]::Now.ToString("yyyy.MM.dd.HHmm"))</Version>
        <Authors>Elringus</Authors>
        <Company>ReWaffle LLC</Company>
        <PackageDescription>JavaScript bindings for naniscript language server.</PackageDescription>
        <RepositoryUrl>https://github.com/naninovel/language</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Bootsharp.Common" Version="0.3.3"/>
        <PackageReference Include="Bootsharp.Inject" Version="0.3.3"/>
        <PackageReference Include="Naninovel.Common.Bindings" Version="2024.10.15.2027"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="../Naninovel.Language/Naninovel.Language.csproj"/>
    </ItemGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Diagnostics.CodeAnalysis.ExcludeFromCodeCoverageAttribute"/>
    </ItemGroup>

</Project>
