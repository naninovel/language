<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <PackageId>Naninovel.Language.Bindings</PackageId>
        <Version>$([System.DateTime]::Now.ToString("yyyy.MM.dd.HHmm"))</Version>
        <Authors>Elringus</Authors>
        <Company>ReWaffle LLC</Company>
        <PackageDescription>JavaScript bindings for naniscript language server.</PackageDescription>
        <RepositoryUrl>https://github.com/Naninovel/Language</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Bootsharp.Common" Version="0.1.0-rc.28"/>
        <PackageReference Include="Bootsharp.DependencyInjection" Version="0.1.0-rc.28"/>
        <PackageReference Include="Naninovel.Common.Bindings" Version="2023.9.28.1853"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="../Naninovel.Language/Naninovel.Language.csproj"/>
    </ItemGroup>

</Project>
